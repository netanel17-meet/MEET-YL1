from turtle import *
import random
cells=[]
for x in range(0,99):
       cell ={"x":random.randint(-50,50), "y":random.randint(-50,50), "dx":0.1, "dy":0.1, "radius":5, "color":random.randint()}
	cell=create_cell(cell)
	cells.append(cell)


while True:
	move_cells(cells)












